#    4.VISUALIZATION REPORT

def char_summary_cal(data):
    col=list(data.columns)
    for i in col:
        if data[i].dtypes=="object":
            print(i)

char_summary_cal(data=tbd)

def char_summary_cal(data):
    col=list(data.columns)
    for i in col:
        if data[i].dtypes=="object":
            abc=pd.DataFrame(data[i].value_counts())
            return abc
            print(i)

char_summary_cal(data=tbd)

tbd["MONTH_YY"].value_counts()

